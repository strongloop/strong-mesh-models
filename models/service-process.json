{
  "name": "ServiceProcess",
  "base": "PersistedModel",
  "properties": {
    "parentPid": {
      "type": "number",
      "required": true,
      "index": true
    },
    "pid": {
      "type": "number",
      "required": true
    },
    "workerId": {
      "type": "number",
      "required": true
    },
    "endpoints": {
      "type": ["object"],
      "default": []
    },
    "isTrackingObjects": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "isProfiling": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "isSnapshotting": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "startTime": {
      "type": "date"
    },
    "stopTime": {
      "type": "date"
    },
    "stopReason": {
      "type": "string"
    }
  },
  "relations": {
    "serviceInstance": {
      "type": "belongsTo",
      "model": "ServiceInstance"
    }
  },
  "acls": []
}

