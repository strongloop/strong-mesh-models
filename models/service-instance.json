{
  "name": "ServiceInstance",
  "base": "PersistedModel",
  "properties": {
    "executorId": {
      "type": "number",
      "index": 1
    },
    "serverServiceId": {
      "type": "number",
      "index": 1
    },
    "groupId": {
      "type": "number",
      "index": 1
    },
    "currentDeploymentId": {
      "type": "string"
    },
    "startTime": {
      "type": "date"
    },
    "restartCount": {
      "type": "number"
    },
    "stats": {
      "type": "object"
    },
    "PMPort": {
      "type": "number"
    },
    "containerVersionInfo":{
      "type": "object"
    },
    "started": {
      "type": "boolean",
      "default": false
    },
    "setSize": {
      "type": "number",
      "default": 0
    },
    "applicationName": {
      "type": "string"
    },
    "npmModules": {
      "type": "object"
    },
    "agentVersion": {
      "type": "string"
    }
  },
  "options": {
    "hidden": ["executorId"]
  },
  "relations": {
    "actions": {
      "type": "hasMany",
      "model": "InstanceAction"
    },
    "processes": {
      "type": "hasMany",
      "model": "ServiceProcess"
    },
    "executor": {
      "type": "belongsTo",
      "model": "Executor"
    },
    "serverService": {
      "type": "belongsTo",
      "model": "ServerService"
    }
  },
  "acls": []
}
